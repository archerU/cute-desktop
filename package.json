{
  "name": "cute-desktop",
  "productName": "视频龙",
  "description": "视频桌面端工具",
  "repository": "git@github.com:archerU/cute-desktop.git",
  "author": "archerU",
  "license": "MIT",
  "version": "1.0.0",
  "main": "main.js",
  "env": "production",
  "scripts": {
    "dev": "webpack-dev-server --progress  --host localhost --port 8888 --contentBase app/app.js --hot",
    "start": "electron .",
    "compile": "yarn clear && yarn compile:updater && yarn webpack-cli --progress --config ./webpack.base.config.js",
    "compile:updater": "rimraf updater/build && tsc -p updater",
    "package": "electron-builder build --config ./electron-builder.json",
    "clear": "rimraf bundles"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^1.4.1",
    "antd": "^4.22.3",
    "electron-updater": "^4.3.9",
    "electron-window-state": "^5.0.3",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "tasklist": "3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.6.4",
    "@types/node-fetch": "^2.6.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "electron": "^20.0.1",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.2.1",
    "typescript": "^4.4.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
